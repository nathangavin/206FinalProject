	public Users() {
		File file = new File(".Users.Data");
		if (file.isFile()) {
			Scanner scanFile = null;
			
			try {
				scanFile = new Scanner(file);
			} catch (FileNotFoundException e) {
				e.printStackTrace();
			}
			
			while (scanFile.hasNextLine()) {
				
			}
		} else {
			try {
				file.createNewFile();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
	}
	
}
